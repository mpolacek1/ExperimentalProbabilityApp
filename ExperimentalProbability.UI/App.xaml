                <ResourceDictionary Source="Properties\ElementsResources\ComboBoxResources.xaml"/>
                <ResourceDictionary Source="Properties\ElementsResources\IntegerUpDownResources.xaml"/>
                <ResourceDictionary Source="Properties\ElementsResources\TextBlockResources.xaml"/>
                <ResourceDictionary Source="Properties\ElementsResources\WindowResources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
